{
  "name": "react-triple-select-box",
  "version": "2.1.4",
  "description": "A simple, dependencies-free and dumb react component to render triple select boxes.",
  "main": "dist/index.js",
  "scripts": {
    "start": "babel ./src -w --out-dir ./dist -s inline --ignore '**/*.test.js' && ./scripts/--copy-files src dist",
    "prepublishOnly": "babel ./src --out-dir ./dist -s inline --ignore '**/*.test.js' && ./scripts/--copy-files src dist",
    "storybook": "rm ./dist && npm run prepublishOnly && start-storybook",
    "test": "jest --watchAll",
    "lint": "eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrocostadev/react-triple-select-box.git"
  },
  "keywords": [
    "react",
    "select-box",
    "triple-select-box"
  ],
  "author": "Pedro Costa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pedrocostadev/react-triple-select-box/issues"
  },
  "homepage": "https://github.com/pedrocostadev/react-triple-select-box#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.10",
    "@testing-library/react": "^9.0.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "files": [
    "dist/*"
  ]
}
